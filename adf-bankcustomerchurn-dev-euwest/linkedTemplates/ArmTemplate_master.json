{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-bankcustomerchurn-dev-euwest"
		},
		"ls_akv_bank_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-bank.vault.azure.net/"
		},
		"ls_dbw_bank_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1208-210551-090epprm"
		},
		"ls_dl_bank_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlbankcustomerchurn.dfs.core.windows.net/"
		},
		"ls_github_bank_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_sql_bank_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-server-bank-euwest.database.windows.net"
		},
		"ls_sql_bank_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-bank-euwest"
		},
		"ls_sql_bank_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "bankadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-bankcustomerchurn-dev-euwest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_github_bank_properties_typeProperties_url": {
						"value": "[parameters('ls_github_bank_properties_typeProperties_url')]"
					},
					"ls_akv_bank_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_bank_properties_typeProperties_baseUrl')]"
					},
					"ls_dl_bank_properties_typeProperties_url": {
						"value": "[parameters('ls_dl_bank_properties_typeProperties_url')]"
					},
					"ls_dbw_bank_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_dbw_bank_properties_typeProperties_existingClusterId')]"
					},
					"ls_sql_bank_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_bank_properties_typeProperties_server')]"
					},
					"ls_sql_bank_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_bank_properties_typeProperties_database')]"
					},
					"ls_sql_bank_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_bank_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}